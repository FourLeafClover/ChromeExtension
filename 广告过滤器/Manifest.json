{
    "name": "广告过滤器",
    "version": "1.0",
    "manifest_version": 2,
    "author": "Frank Zhang",
    "browser_action": {
        "default_popup": "background.html"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["contentscript.js", "jquery.min.js"]
    }]
}